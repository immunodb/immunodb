---
interface Ref {
  title: string;
  url: string;
}

const { refs } = Astro.props as { refs: Ref[] };
---

<h2>References</h2>
<hr />
<ul>
  {
    refs.map(() => (
      <li>
        Article - Author - <a href="https://pubmed.ncbi.nlm.nih.gov/">Pubmed</a>
      </li>
    ))
  }
</ul>

<style>
  h2 {
    font-size: 20px;
    margin-top: 20px;
    font-family: Times, serif;
  }
</style>
