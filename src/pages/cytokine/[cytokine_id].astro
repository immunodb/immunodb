---
import cytokines from "../../../data/cytokines.json";
import Page from "../../layouts/page.astro";

export async function getStaticPaths() {
  return cytokines.map((cytokine) => ({
    params: { cytokine_id: cytokine.cytokine_id },
    props: { cytokine },
  }));
}

let { cytokine } = Astro.props;
cytokine.infobox = [
  { key: "Class", value: [{ name: "Cytokine", id: undefined }] },
  { key: "Produced By", value: cytokine.produced_by },
  { key: "Acts On", value: cytokine.stimulates },
];
---

<Page data={cytokine} />
