---
import markers from "../../../data/markers.json";
import Page from "../../layouts/page.astro";

export async function getStaticPaths() {
  return markers.map((marker) => ({
    params: { marker_id: marker.marker_id },
    props: { marker },
  }));
}

let { marker } = Astro.props;
marker.infobox = [
  { key: "Class", value: [{ name: "Surface Marker", id: undefined }] },
  { key: "Found On", value: marker.found_on },
];
---

<Page data={marker} />
